<?xml version="1.0" encoding="UTF-8"?>
<Configuration >  
    <Appenders>  
        <Console name="Console" target="SYSTEM_OUT">  
            <PatternLayout pattern="[%-5level] %d{HH:mm:ss} %logger{36}- %msg%n" />
        </Console>  
    </Appenders>  
    
    <Loggers>  
        <Root level="WARN">
            <AppenderRef ref="Console" />  
        </Root>  
    </Loggers>  
</Configuration>